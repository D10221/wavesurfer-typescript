{
  "name": "@types/wavesurfer",
  "version": "1.0.0",
  "description": "Typings for https://wavesurfer-js.org",
  "typings": "src/index.d.ts",
  "scripts": {
    "globals": "npm install -g rimraf typescript mocha electron electron-mocha typescript-formatter tslint",
    "lint": "tslint src/*.ts",
    "format": "tsfmt src/**/*.ts -r",
    "link": "npm link mocha electron-mocha electron",
    "test": "mocha ./test.js",
    "build": "tsc -p .",
    "clean": "rimraf ./built",
    "rebuild": "npm run clean && npm run build",
    "start": "electron .",
    "debug": "npm run rebuild && SET DEBUG=wavesurfer-typescript* && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d10221/wavesurfer-typescript"
  },
  "author": {
    "name": "Mr. D",
    "email": "danieliozzi@gmail.com",
    "url": "https://github.com/D10221"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/D10221/wavesurfer-typescript/issues"
  },
  "homepage": "https://github.com/D10221/wavesurfer-typescript#readme",
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/electron": "^1.4.37",
    "@types/jsdom": "^2.0.30",
    "@types/mocha": "^2.2.41",
    "debug": "^2.6.6",
    "jsdom": "^9.0.0",
    "tslint": "^5.2.0",
    "tslint-config-typings": "^0.3.1",
    "typescript": "^2.3.2",
    "typescript-formatter": "^5.1.3",
    "wavesurfer.js": "^2.0.0-beta01",
    "web-audio-test-api": "^0.5.2"
  },
  "dependencies": {}
}
